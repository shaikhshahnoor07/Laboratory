//Write a program to display the following pattern


public class Pattern_9 {

	public static void main(String[] args) {
		int i,j;
		char ch='A'; //Declare Character
		for(i=1;i<=5;i++)
		{
			for(j=1;j<=i;j++) //Increasing char
			{
				System.out.print(ch+"");
				ch++;
			}

			System.out.println();
		}

	}

}
